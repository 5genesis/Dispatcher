          {
            "BearerToken": []
          }
        ],
        "tags": [
          "MANO"
        ],
        "summary": "Add a VNFD or new VNFD version to the repository",
        "description": "This request accepts 1 to many VNFs files. The file first is validated with the images dependencies and syntactic analysis.",
        "operationId": "addVNFD",
        "consumes": [
          "multipart/form-data"
        ],
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "file",
            "in": "formData",
            "description": "VNFD Package",
            "required": true,
            "type": "file"
          },
          {
            "name": "visibility",
            "in": "formData",
            "description": "visibility of the NSD",
            "required": true,
            "type": "boolean"
          }
        ],
        "responses": {
          "200": {
            "description": "VNFs Added",
            "schema": {
              "type": "object",
              "properties": {
                "VNFs": {
                  "type": "object",
                  "properties": {
                    "hackfest_1_vnfd_fixed.tar.gz": {
                      "type": "string",
                      "example": "VNF added"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "VNFs not uploaded",
            "schema": {
              "type": "object",
              "properties": {
                "VNFs": {
                  "type": "object",
                  "properties": {
                    "hackfest_1_vnfd_fixed.tar.gz": {
                      "type": "string",
                      "example": "VNFD with this version already exists"
                    }
                  }
                }
              }
            }
          },
          "500": {
            "description": "Internal server error - Network problems"
          }
        }
      },
      "get": {
        "security": [
          {
            "basicAuth": []
          },
          {
            "BearerToken": []
          }
